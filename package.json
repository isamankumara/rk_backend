{
  "name": "a_life_lived_backend",
  "version": "1.0.0",
  "description": "Mobile Application",
  "engines": {
    "node": "12.22.x"
  },
  "main": "preIndex.js",
  "scripts": {
    "nodemon": "nodemon -e \"ts,json,js\" -w \"./src\" -x \"npm run-script typescript:compile && npx kill-port 3000 && npm run dev\"",
    "dev": "cross-env NODE_ENV=development PORT=3000 DISABLE_LOGGING=true keystone dev --entry=./all-dist/src/index.js",
    "build": "cross-env NODE_ENV=production keystone build --entry=./all-dist/src/index.js",
    "start": "cross-env NODE_ENV=production CONFIG_ENV=prod PRINT_VARS=true node ./all-dist/src/environment.js",
    "start:local": "cross-env NODE_ENV=development keystone dev",
    "test:unit": "jest ./__tests__/ --runInBand --forceExit",
    "test:unit:ci": "jest ./__tests__/ --runInBand --forceExit --ci --reporters='default' --testTimeout=10000",
    "test:e2e": "cross-env TEST_REDIS=true jest ./e2e/controllers/playlistController.test.ts --runInBand --forceExit --testTimeout=10000",
    "test:e2e:debug": "cross-env NODE_OPTIONS=--inspect-brk=0.0.0.0:9229 TEST_REDIS=true jest ./all-dist/__tests__/e2e/controllers/noauthStoryController.test.js --runInBand --forceExit",
    "test:integration": "cross-env VERSION=20210717110440793105335 BACKEND_BASE_ROUTE=https://alifelived.ts.r.appspot.com TEST_REDIS=true jest ./integration/integration.test.ts --forceExit",
    "test:load:bulk": "jest ./__tests__/load.bulk.test.js --forceExit",
    "test:load:story": "cross-env BACKEND_BASE_ROUTE=https://alifelived.ts.r.appspot.com jest ./scripts/loadTests/load.story.test.js --forceExit",
    "test": "jest",
    "lint": "eslint  --ext .js,.ts,.tsx",
    "typescript:compile": "rm -rf ./all-dist && npm run lint && tsc",
    "typescript:clean": "rm -rf ./all-dist",
    "generate-env": "sh ./scripts/generateEnv.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aLifeLived/aLifeLivedBackend.git"
  },
  "author": "Max Kelly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aLifeLived/aLifeLivedBackend/issues"
  },
  "homepage": "https://github.com/aLifeLived/aLifeLivedBackend#readme",
  "dependencies": {
    "@babel/runtime": "^7.18.9",
    "@google-cloud/debug-agent": "^5.1.3",
    "@google-cloud/secret-manager": "^3.4.0",
    "@hapi/joi": "^17.1.1",
    "@jamify/redis-streams": "^1.3.0",
    "@keystonejs/adapter-mongoose": "^9.0.3",
    "@keystonejs/app-admin-ui": "^7.3.2",
    "@keystonejs/app-graphql": "^6.1.0",
    "@keystonejs/auth-password": "^5.1.14",
    "@keystonejs/email": "^5.3.0",
    "@keystonejs/fields": "^17.0.0",
    "@keystonejs/file-adapters": "^7.0.4",
    "@keystonejs/keystone": "^19.2.0",
    "@keystonejs/list-plugins": "^7.1.5",
    "@types/supertest": "^2.0.11",
    "aws-sdk": "^2.702.0",
    "axios": "^0.21.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "caniuse-lite": "^1.0.30000974",
    "connect-redis": "^5.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "cross-env": "^7.0.0",
    "cryptr": "^6.0.2",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express-session": "^1.17.1",
    "fluent-ffmpeg": "^2.1.2",
    "fs": "0.0.1-security",
    "google-spreadsheet": "^3.0.13",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mp3-duration": "^1.1.0",
    "multer": "^1.4.2",
    "multer-s3": "^2.9.0",
    "node-cron": "^2.0.3",
    "nodemailer": "^6.5.0",
    "path": "^0.12.7",
    "redis": "^3.0.2",
    "redis-streams": "^1.1.0",
    "rimraf": "^3.0.2",
    "url": "^0.11.0",
    "uuid": "^8.3.2",
    "wav": "^1.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/eslint-parser": "^7.18.9",
    "@babel/plugin-transform-runtime": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@types/express": "^4.17.13",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.3.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^8.21.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "loadtest": "^5.1.2",
    "nodemon": "^2.0.19",
    "prettier": "^2.2.1",
    "redis-cli": "^2.0.0",
    "supertest": "^6.1.1",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test:unit",
      "pre-commit": "npm run lint"
    }
  }
}
